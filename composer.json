{
    "name": "mahdi-morovati/mongodb-passport",
    "description": "A Package To Integrate Passport With jenssegers/laravel-mongodb Package",
    "keywords": ["laravel", "passport", "oauth", "mongodb"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/passport/issues",
        "source": "https://github.com/laravel/passport"
    },
    "authors": [
        {
            "name": "Mahdi Morovati",
            "email": "morovati155@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "firebase/php-jwt": "^5.0",
        "illuminate/auth": "^6.18.31|^7.22.4",
        "illuminate/console": "^6.18.31|^7.22.4",
        "illuminate/container": "^6.18.31|^7.22.4",
        "illuminate/contracts": "^6.18.31|^7.22.4",
        "illuminate/cookie": "^6.18.31|^7.22.4",
        "illuminate/database": "^6.18.31|^7.22.4",
        "illuminate/encryption": "^6.18.31|^7.22.4",
        "illuminate/http": "^6.18.31|^7.22.4",
        "illuminate/support": "^6.18.31|^7.22.4",
        "laminas/laminas-diactoros": "^2.2",
        "lcobucci/jwt": "^3.4|^4.0",
        "league/oauth2-server": "^8.2.3",
        "nyholm/psr7": "^1.0",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "^2.0",
        "laravel/passport": "~9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.4|^5.0",
        "phpunit/phpunit": "^8.5|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
